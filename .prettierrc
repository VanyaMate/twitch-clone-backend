{
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": true,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
        "classProperties",
        "decorators-legacy",
        "typescript"
    ],
    "importOrders": [
        "<THIRD_PARTY_MODULE>",
        "^@/(.*)$",
        "^../(.*)",
        "^./(.*)"
    ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}